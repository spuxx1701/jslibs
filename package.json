{
  "name": "jslibs",
  "version": "1.0.0",
  "description": "A collection of libraries a maintain for my personal web stack.",
  "license": "MIT",
  "private": true,
  "type": "module",
  "author": {
    "name": "spuxx",
    "email": "hi@spuxx.dev",
    "url": "https://spuxx.dev"
  },
  "homepage": "https://github.com/spuxx1701/jslibs/blob/main/README.md",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev": "concurrently \"pnpm dev:js-utils\" \"pnpm dev:browser-utils\" \"pnpm dev:nest-utils\" \"pnpm dev:nest-testing\" \"pnpm dev:app:react\" \"pnpm dev:app:nest\"",
    "dev:js-utils": "cd packages/js-utils && pnpm dev",
    "dev:browser-utils": "cd packages/browser-utils && pnpm dev",
    "dev:nest-utils": "cd packages/nest-utils && pnpm dev",
    "dev:nest-testing": "cd packages/nest-testing && pnpm dev",
    "app:react": "cd apps/react && pnpm dev",
    "app:nest": "cd apps/nest && pnpm dev",
    "publish:test": "pnpm -r publish --dry-run --no-git-checks",
    "test": "pnpm test:vitest",
    "test:vitest": "vitest run --coverage",
    "check": "pnpm types && pnpm run lint && pnpm run prettier-check",
    "types": "pnpm -r types",
    "lint": "eslint . --ext ts,tsx",
    "prettier-check": "prettier --check .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@deno/vite-plugin": "^1.0.0"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "1.1.0",
    "@nestjs/cli": "10.4.2",
    "@nestjs/testing": "10.3.10",
    "@swc/cli": "0.4.0",
    "@swc/core": "1.7.22",
    "@types/express": "4.17.21",
    "@types/node": "22.5.2",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitest/coverage-v8": "2.0.5",
    "concurrently": "9.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "msw": "2.6.0",
    "prettier": "3.3.3",
    "release-please": "16.12.0",
    "typescript": "5.5.4",
    "unplugin-swc": "1.5.1",
    "vite": "5.4.8",
    "vite-plugin-dts": "4.2.1",
    "vite-tsconfig-paths": "5.1.2",
    "vitest": "2.0.5"
  }
}
