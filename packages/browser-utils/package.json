    "test": "pnpm run test:vitest",
    "test:vitest": "vitest run --coverage"
  },
  "dependencies": {
    "@spuxx/js-utils": "1.3.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "3.0.5",
